@model EmptyProject.ViewModels.User.LoginUserVM

@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

<form method="post">
    <label for="login">Login:</label>
    <input type="text" name="login" />
    <br />

    <label for="password">Password:</label>
    <input type="password" name="password" />
    <br />
    
    <input type="submit" value="Login"/>
</form>
